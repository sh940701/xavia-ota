# Xavia OTA runtime env template for AWS
# Copy to .env.aws (do not commit real values)

# Default profile: AWS infra + Supabase DB/Storage
HOST=https://ota-dev.example.com
NODE_ENV=production
PORT=3000

BLOB_STORAGE_TYPE=supabase
DB_TYPE=supabase

SUPABASE_URL=
SUPABASE_API_KEY=
SUPABASE_BUCKET_NAME=expo-updates

ADMIN_PASSWORD=
ADMIN_SESSION_SECRET=
ADMIN_SESSION_MAX_AGE_SECONDS=43200
UPLOAD_KEY=
PRIVATE_KEY_BASE_64=

